
<div id="snippet-grid-grid">





<form action="/" method="get" id="frm-grid-form" class="ajax grido table-responsive bootstrap" data-grido-refresh-handler="/?action=default&amp;do=grid-refresh&amp;presenter=Test">

<table id="grid" class="table table-striped table-hover">
    <thead>
        <tr class="head">
            <th class="checker" rowspan="2">
                <input type="checkbox" title="Invert">
            </th>
                <th class="column grid-header-firstname">
                        <span>Firstname</span>
                </th>
                <th class="column grid-header-surname">
                        <a class="ajax" href="/?grid-sort%5Bsurname%5D=asc&amp;action=default&amp;do=grid-sort&amp;presenter=Test">Surname</a>
                </th>
                <th class="column grid-header-gender">
                        <a class="ajax" href="/?grid-sort%5Bgender%5D=asc&amp;action=default&amp;do=grid-sort&amp;presenter=Test">Gender</a>
                </th>
                <th class="column grid-header-birthday">
                        <a class="ajax" href="/?grid-sort%5Bbirthday%5D=asc&amp;action=default&amp;do=grid-sort&amp;presenter=Test"><i class="icon-birthday"></i></a>
                </th>
                <th class="actions center col-xs-1">Actions</th>
        </tr>
        <tr class="filter inner">
                    <th class="grid-filter-firstname">
                            <input type="text" name="filters[firstname]" class="text form-control" id="frm-grid-form-filters-firstname">
                    </th>
                    <th class="grid-filter-surname">
                            <input type="text" name="filters[surname]" class="text form-control" id="frm-grid-form-filters-surname">
                    </th>
                    <th class="grid-filter-gender">
                            <input type="text" name="filters[gender]" class="text form-control" id="frm-grid-form-filters-gender">
                    </th>
                    <th class="grid-filter-birthday">
                            <input type="text" name="filters[birthday]" class="text date form-control" autocomplete="off" id="frm-grid-form-filters-birthday">
                    </th>

            <th class="buttons">
            </th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <td colspan="6">
                <span class="operations"  title="Select some row">
                    <select name="operations[operations]" class="form-control" id="frm-grid-form-operations-operations"><option value="">Selected...</option><option value="print">Print</option></select>
                    <input type="submit" name="buttons[operations]" class="hide" value="OK">
                </span>
<span class="paginator">
    <ul>
        <li>
                <a class="disabled btn btn-default btn-sm"><i class="fa fa-backward"></i></a>
        </li>
        <li>
                <a class="disabled btn btn-default btn-sm"><i class="fa fa-chevron-left"></i></a>
        </li>
        <li>
            <input type="text" class="form-control" placeholder="Page 1 z 13" data-grido-current="1" min="1" max="13" data-grido-link="/?grid-page=0&amp;action=default&amp;do=grid-page&amp;presenter=Test">
        </li>
        <li>
                <a class="ajax btn btn-default btn-sm" title="Next" href="/?grid-page=2&amp;action=default&amp;do=grid-page&amp;presenter=Test"><i class="fa fa-chevron-right"></i></a>
        </li>
        <li>
                <a class="ajax btn btn-default btn-sm" title="Next" href="/?grid-page=13&amp;action=default&amp;do=grid-page&amp;presenter=Test"><i class="fa fa-forward"></i></a>
        </li>
    </ul>
</span>
                <span class="right">
